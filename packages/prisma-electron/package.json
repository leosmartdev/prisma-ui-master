    "electron-packager": "^12.0.0",
    "electron-winstaller": "^2.6.4",
    "file-loader": "^3.0.1",
    "i18next-conv": "^3.0.1",
    "ignore-loader": "^0.1.1",
    "inject-loader": "^4.0.1",
    "jsxgettext": "^0.11.0",
    "mock-fs": "^4.8.0",
    "redux-mock-store": "^1.3.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  }
}
