{
  "name": "@prisma/electron",
  "productName": "PRISMA",
  "description": "Electron Client for PRISMA systems.",
  "version": "1.8.1",
  "main": "main/main.js",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/orolia/prisma.git"
  },
  "scripts": {
    "package:build": "cross-env NODE_ENV=production bash -c 'make/make-build && make/make-brand generic'",
    "package:macOS": "yarn package:build && bash -c 'make/make-package macOS'",
    "package:windows": "yarn package:build && bash -c 'make/make-package windows'",
    "create-installer": "bash -c 'make/make-installer'",
    "create-installer:macOS": "bash -c 'make/make-installer macOS'",
    "create-installer:windows": "bash -c 'make/make-installer windows'",
    "clean": "make/make-clean",
    "dist": "yarn package:build && bash -c 'make/make-package'",
    "dist:clean": "make/make-clean dist",
    "dist:windows": "yarn package:windows && yarn create-installer:windows",
    "dist:macOS": "yarn package:macOS && yarn create-installer:macOS",
    "build": "webpack --config webpack.config.development.js",
    "build:production": "cross-env NODE_ENV=production yarn build:application:production",
    "build:application:production": "webpack --config webpack.config.production.js",
    "translate-pot": "make/translate-pot",
    "translate-json": "make/translate-json",
    "start:production": "cross-env NODE_ENV=production electron . --serverEnv=production",
    "start:test": "cross-env NODE_ENV=test electron . --serverEnv=test",
    "start": "cross-env NODE_ENV=development electron . --devtools",
    "start:devServer": "webpack-dev-server --open --config webpack.config.web.js --mode development",
    "start:local": "cross-env NODE_ENV=development electron . --serverEnv=development --devtools --config=http://localhost:8081/api/v2/config.json",
    "test": "CURRENT_DIR=`pwd` && cd ../../ && yarn test ${CURRENT_DIR}",
    "storybook": "start-storybook -p 6006 -s ./src/",
    "build-storybook": "build-storybook -s ./src/ --output-dir dist/storybook-static --quiet"
  },
  "dependencies": {
    "@date-io/date-fns": "1.3.11",
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/pickers": "^3.3.10",
    "@material-ui/styles": "^4.0.0",
    "@prisma/map": "1.8.1",
    "@prisma/ui": "1.8.1",
    "ajv": "^5.3.0",
    "ajv-i18n": "^2.2.0",
    "autosuggest-highlight": "^3.1.1",
    "blueimp-md5": "^2.12.0",
    "clone": "^2.0.0",
    "color": "^3.1.0",
    "date-fns": "2.11.1",
    "dms-conversion": "^3.1.1",
    "electron": "^1.8.1",
    "electron-debug": "^1.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "enzyme": "^3.9.0",
    "flag-icon-css": "^3.3.0",
    "font-awesome": "^4.6.3",
    "formik": "^1.5.2",
    "i18next": "^3.1.0",
    "i18next-sync-fs-backend": "^0.1.0",
    "immutable": "^3.8.2",
    "iso-country-codes": "^0.0.5",
    "lodash": "^4.17.4",
    "loglevel": "^1.5.1",
    "macaddress": "^0.5.2",
    "moment": "^2.19.4",
    "moment-timezone": "^0.5.14",
    "openlayers": "^4.4.2",
    "ordinal": "^1.0.3",
    "papaparse": "^5.1.1",
    "pdfjs-dist": "2.5.207",
    "pino": "^5.12.2",
    "pino-pretty": "^2.6.0",
    "prop-types": "^15.5.7",
    "react": "^16.8.6",
    "react-autosuggest": "^9.3.2",
    "react-color": "^2.2.2",
    "react-dom": "^16.8.6",
    "react-dropzone": "^4.2.1",
    "react-fontawesome": "^1.2.0",
    "react-i18next": "^7.3.1",
    "react-infinite": "^0.13.0",
    "react-input-mask": "^2.0.4",
    "react-pdf": "5.1.0",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-virtualized": "^9.15.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.1.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.83.0",
