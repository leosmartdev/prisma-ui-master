{
  "name": "@prisma/workspace",
  "version": "1.8.1",
  "description": "Workspace for PRISMA UI and Clients",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/react-scripts",
      "**/react-scripts/**"
    ]
  },
  "homepage": "https://documentation.mcmurdo.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/orolia/prisma-ui.git"
  },
  "bugs": {
    "url": "https://mcmurdo.atlassian.net/browse/PC2"
  },
  "scripts": {
    "build": "yarn build:ui && yarn build:map && yarn build:electron",
    "build:electron": "cd packages/prisma-electron && yarn build",
    "build:map": "cd packages/prisma-map && yarn build",
    "build:ui": "cd packages/prisma-ui && yarn build",
    "build:storybook": "yarn build:storybook:ui && yarn build:storybook:map && yarn build:storybook:electron",
    "build:storybook:electron": "cd packages/prisma-electron && yarn build-storybook",
    "build:storybook:map": "cd packages/prisma-map && yarn build-storybook",
    "build:storybook:ui": "cd packages/prisma-ui && yarn build-storybook",
    "build:docs": "cd docs && make",
    "dist:clean": "yarn dist:clean:electron && yarn dist:clean:map && yarn dist:clean:ui && rm -rf ./dist && rm -rf ./node_modules && rm -rf ./coverage/",
    "dist:clean:electron": "cd packages/prisma-electron && yarn dist:clean",
    "dist:clean:map": "cd packages/prisma-map && yarn dist:clean",
    "dist:clean:ui": "cd packages/prisma-ui && yarn dist:clean",
    "start": "cd packages/prisma-electron && yarn start",
    "start:docs": "cd docs && make serve",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coverage:codecov": "yarn test:coverage && codecov"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.3",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.3",
    "@babel/standalone": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@storybook/addon-actions": "^5.0.8",
    "@storybook/addon-centered": "^5.0.8",
    "@storybook/addon-info": "^5.0.8",
    "@storybook/addon-knobs": "^5.0.8",
    "@storybook/addon-links": "^5.0.8",
    "@storybook/addon-notes": "^5.0.8",
    "@storybook/addon-options": "^5.0.8",
    "@storybook/addon-storysource": "^5.0.8",
    "@storybook/addon-viewport": "^5.0.8",
    "@storybook/addons": "^5.0.8",
    "@storybook/cli": "^5.0.8",
    "@storybook/react": "^5.0.8",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-module-resolver": "^3.1.1",
    "codecov": "^3.2.0",
    "cross-env": "^3.1.3",
    "deep-freeze": "^0.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "fs-extra": "^7.0.1",
    "jest": "^24.7.1",
    "lerna": "^3.13.4",
    "prettier": "1.17.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.4.0",
    "rollup": "^1.11.3",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-eslint": "^5.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-size-snapshot": "^0.8.0",
    "rollup-plugin-terser": "^4.0.4"
  }
}
